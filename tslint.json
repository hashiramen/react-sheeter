{
    "extends": "tslint:recommended",
    "rules": {
        "curly": false,
        "max-line-length": false,
        "ordered-imports": false,
        "no-consecutive-blank-lines": false,
        "no-unused-variable": true,
        "no-trailing-whitespace": false,
        "variable-name": false,
        "whitespace": false,
        "only-arrow-functions": false,
        "member-access": false,
        "prefer-for-of": false,
        "no-console": false
    }
}